<div class="span9 offset2">
  <%= render 'tool_types/nav' %>
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
        <th><%= ServiceVehicle.human_attribute_name('number') %></th>
      </tr>
    </thead>
    <tbody>
      <% @service_vehicles.each do |service_vehicle| -%>
        <tr>
          <td><%= link_to service_vehicle.number, tool_assignments_service_vehicle_path(service_vehicle) %></td>
        </tr>
      <% end -%>
    </tbody>
  </table>
  <%= paginate @service_vehicles %>
</div>
