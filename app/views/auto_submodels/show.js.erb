$("#auto_model_brand_id").val("<%= @auto_submodel.auto_model.auto_brand.id %>")
$("#auto_auto_model, #auto_submodel_model_id").html("<%= escape_javascript( options_for_select(@auto_submodel.auto_model.auto_brand.auto_models.asc(:name).map {|p| [p.name, p.id]})) %>").val("<%= @auto_submodel.auto_model.id %>")
$("#ajax-message").text("<%= @notify_message %>");

$("#select-parts").html("<%= escape_javascript(render("parts", parts: @auto_submodel.parts_by_type)) %>");
$("#select-service-types").html("<%= escape_javascript(render("service_types")) %>");

$("#auto_submodels").html("<%= escape_javascript(render("auto_submodels")) %>");
$("#order_auto_submodel_id").val("<%= @auto_submodel.id %>");
$("#models_and_parts_info").html("<%= escape_javascript(render('auto_submodels/models_has_same_displacement', asm: @auto_submodel)) %>");

city_id = $("#order_city_id").val()
$.each($(".order-storehouse-city"), function( i, val ){
    if(city_id == $(val).val()) {
        $(val).parent().show()
    }else{
        $(val).parent().hide()
    }
    
});
