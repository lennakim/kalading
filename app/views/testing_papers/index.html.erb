<div class="container">

  <div class="row">
    <div class="span12">
      <%= link_to "新建考试", new_testing_paper_path, class: "btn" %>
    </div>
  </div>

  <div class="row">
    <div class="span12">
      <table class="table">
        <tr>
          <td>标题</td>
          <td>操作</td>
        </tr>
        <% collection.each do |testing_paper| %>
          <tr>
            <td><%= link_to testing_paper.title, testing_paper_path(testing_paper) %></td>
            <td>
              <%= link_to "编辑", edit_testing_paper_path(testing_paper) %>
              <%= link_to "删除", testing_paper_path(testing_paper), method: :delete %>
            </td>
          </tr>
        <% end %>
      </table>
    </div>
  </div>
</div>
