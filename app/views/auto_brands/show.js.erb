$("#auto_submodel_model_id").html("<%= escape_javascript( options_for_select(@auto_brand.auto_models.map {|p| [p.name, p.id]})) %>").val("<%= @auto_brand.auto_models.first.name %>").prop('selectedIndex',0);
$("#auto_auto_submodel_id, #order_auto_submodel_id").html("<%= escape_javascript( options_for_select(@auto_brand.auto_models.first.auto_submodels.map {|p| [p.name, p.id]})) %>").prop('selectedIndex',0);
$("#service_type_auto_model_id").html("<%= escape_javascript( options_for_select(@auto_brand.auto_models.map {|p| [p.name, p.id]})) %>");
$("#auto_submodels").html("<%= escape_javascript(render("auto_submodels/auto_submodels")) %>");
$("#auto_submodel_year").val("<%= t(:all) %>");
<% if mobile_device? %>
$("#auto_submodel_model_id").selectmenu('refresh');
$("#order_auto_submodel_id").selectmenu('refresh');
<% end %>

