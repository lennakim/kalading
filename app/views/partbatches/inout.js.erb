<% if @error %>
    $("#error-div").html("<%= escape_javascript(render 'partbatches/error') %>");
<% else %>
    $("#<%= @partbatch.id %>").text("<%= @partbatch.remained_quantity %>");
    $('#modal-window').modal('hide').html("");
    $("#history_trackers").html("<%= escape_javascript(render("storehouses/history")) %>");
    $("#history_trackers ul li:first-child").fadeOut().fadeIn();
<% end %>

 