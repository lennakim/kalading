<%- model_class = PartTransfer -%>
<div class="page-header">
  <h1><%= t 'part_transfers' %></h1>
</div>
<%= render "search_form" %>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= t(:part_number) %></th>
      <th><%= t(:quantity) %></th>
      <th><%= t(:from) %></th>
      <th><%= t(:to) %></th>
      <th><%= t(:created_at) %></th>
      <th><%= t(:created_by) %></th>
      <th><%= t(:updated_by) %></th>
      <th><%= t(:state) %></th>
      <th><%= t 'actions' %></th>
    </tr>
  </thead>
  <tbody>
    <% @part_transfers.each do |part_transfer| %>
      <tr id="tr-<%= part_transfer.id.to_s %>">
        <%= render 'row', part_transfer: part_transfer %>
      </tr>
    <% end %>
  </tbody>
</table>